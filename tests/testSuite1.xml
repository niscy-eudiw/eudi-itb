<?xml version="1.0" encoding="UTF-8"?>
<testsuite id="verificationTest" xmlns="http://www.gitb.com/tdl/v1/" xmlns:gitb="http://www.gitb.com/core/v1/">
    <metadata>
        <gitb:name>Verification tests</gitb:name>
        <gitb:description>Test suite to validate wallet is working with reference verifier.</gitb:description>
        <gitb:version>1.0</gitb:version>
    </metadata>
	<!-- 
		Define the actors involved in the test suite. The test cases referencing an actor with the role of SUT (System Under Test)
		must be passed to complete conformance testing.

		Depending on options selected during the test suite's upload, the actor definitions will be created or updated in the target specification.
	-->
    <actors>
		<gitb:actor id="verifier">
            <gitb:name>Verifier</gitb:name>
            <gitb:desc>Verifier</gitb:desc>
        </gitb:actor>
        <gitb:actor id="simulatedInitTransaction">
            <gitb:name>Simulated Init Transaction</gitb:name>
            <gitb:desc>Simulated Init Transaction.</gitb:desc>
        </gitb:actor>
        <gitb:actor id="wallet">
            <gitb:name>Wallet</gitb:name>
            <gitb:desc>Wallet SUT.</gitb:desc>
        </gitb:actor>
    </actors>
	<!-- 
		The list of included test cases (referenced using their ID). The ordering of the test cases here defines their execution
		order when running the test suite.
	-->
    <testcase id="verify_pid_qr_from_verifier"/>
    <testcase id="verify_pid_qr_from_verifier_mso_mdoc"/>
    <testcase id="verify_mdl_qr_from_verifier_mso_mdoc"/>
    <testcase id="verify_mdl_age_qr_from_verifier_mso_mdoc"/>
    <testcase id="verify_mdl_and_pid_from_verifier_mso_mdoc"/>
    <testcase id="verify_pid_sdjwt_remote_presentation_status_list"/>
    <testcase id="verify_pid_mdoc_no_trusted_iaca_cert"/>
</testsuite>